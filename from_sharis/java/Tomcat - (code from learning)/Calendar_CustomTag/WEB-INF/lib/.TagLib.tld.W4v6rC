<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
	"http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<!-- template test -->

<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>aCalendar</short-name>
    <uri></uri>
    <description>A blank tag library template. </description>
    <tag>
        <name>Calendar</name>
        <tag-class>Calendar.TagCal</tag-class>
        <body-content>EMPTY</body-content>
        <attribute>
            <name>day</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>month</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>year</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>out</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>dateString</name>
            <required>false</required>
        </attribute>
    </tag>
</taglib>
