<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>
  <servlet>
    <servlet-name>aCalendar</servlet-name>
    <servlet-class>Calendar.TagCal</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>aCalendar</servlet-name>
    <url-pattern>/TagCal</url-pattern>
  </servlet-mapping>
  <taglib>
    <taglib-uri>/TagCal</taglib-uri>
    <taglib-location>/WEB-INF/lib/TagLib.tld</taglib-location>
  </taglib>
</web-app>
