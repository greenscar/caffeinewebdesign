	background-color: #cccccc;
	text-align: right;
   border: 1px;
}
input.itemCostEven
{
	width: 60px;
	padding-bottom: 0px; 
	color: #000000; 
	font-weight: normal; 
	font-size: 12px; 
	background-color: #ffffff;
	text-align: right;
   border: 0px;
}
input.qty
{
	width: 33px;
	padding-bottom: 0px; 
	color: #000000; 
	font-weight: bold; 
    font-family: Verdana, Serif; 
	font-size: 12px; 
	background-color: #FFFFFF;
	text-align: right;
}
input.goTo
{
   font-family: Verdana, Serif; 
	font-size: 12px; 
	font-weight: bold; 
}
select.siteList
{
    border: 1px #000000 solid; 
    color: #000;
    width: 200px;
    height: 400px;
}
select.nextCat
{
   position: absolute;
   left: 50%;
   width: 200px;
}
input.submitOrder
{
   position: absolute;
   width: 20%;
   right: 2%;
   font-family: Verdana, Serif; 
	font-size: 12px; 
	font-weight: bold; 
}
