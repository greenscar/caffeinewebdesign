body 
{
    margin: 10px 10px 10px 10px;
    padding: 0px;
    background-color: #999999; 
    font-family: Verdana, Arial, Helvetica, sans-serif; 
    font-weight: normal;
    font-size: 12px;
    color: #fff;
}

div.mmBody
{
    position: absolute;
    top: 50px;
    width: 780px;
    z-index: 5;
    padding-top: 0px;
    padding-bottom: 0px;
    margin: 0px 0px 0px 15px;
    border: 2px #FFFFFF solid;
}

table.orders
{
    width: 100%;
    background-color: #000000;
    padding: 0px;
    border-width: 0pt;
    border-collapse: collapse;
}

table.orders td
{
    background-color: #FFF;
    text-align: left;
    border: 1px #fff solid;
}
table.orders td.title
{
    background-color: #000;
    padding: 5px;
    font-size: 20px;
    font-weight: bold;
}

table.orders td.subTitle
{
    background-color: #555;
    padding: 5px;
    font-size: 15px;
    font-weight: bold;
}

table.orders td.column_name_bar
{
    background-color: #000;
    padding: 1px;
}

table.orders td.column_name
{
    background-color: #000000;
    color: #FFFFFF;
    padding: 0px 1px 0px 1px;
    border: 0px #000 solid;
    text-align: left;
    font-size: 15px;
    font-weight: bold;
}

table.orders td.dataWhite
{
    background-color: #FFFFFF;
    color: #000000;
    font-size: 12px;
    font-weight: normal;
}

table.orders td.dataGrey
{
    background-color: #000000;
    color: #FFFFFF;
    font-size: 12px;
    font-weight: normal;
}

h1.category
{
    color: black;
    font-size: 40px;
    border: 0px #000000 solid; 
    text-align: center;
    width: 100%;
    margin: 0px 3% 0px 3%;
}
h3.subCategory
{
    color: black;
    font-size: 25px;
    border: 0px #000000 solid; 
    text-align: center;
    width: 100%;
    margin: 10px 0px 10px 0px;
}


span.orderInfo
{
    color: black;
    padding: 5px;
    font-size: 12px;
    color: black;
    border: 0px #fff solid;
    margin: 0px 60px 0px 0px;
}

/********************************************************
 * DISPLAY THE CATALOG CONTENTS
 *******************************************************/
table.orders table.itemList
{
    text-align: center;
    font-size: 11px;
    width: 100%;
    background-color: #666;
    border: 0px;
    border-collapse: collapse;
    border-spacing: 0px;
    z-index: 10;
}

table.orders table.itemList th
{
    background-color: #666;
    padding: 4px;
    border: 1px;
    border-bottom: 1px #fff solid;
    font-size: 13px;
    font-weight: bold;
}
table.orders table.itemList td
{
    font-size: 11px;
    font-weight: normal;
    color: black;
    border: 0px #fff solid;
    /*
    padding: 1px 0px 1px 0px;
    */
}
table.orders table.itemList td.odd
{
    background-color: #CCC;
}
table.orders table.itemList td.even
{
    background-color: #FFF;
}
table.orders td.dataWhite
{
    background-color: #FFFFFF;
    color: #000000;
}
/********************************************************
 * END DISPLAY THE CATALOG CONTENTS
 *******************************************************/


