function checkTeacher(form)
{
    for(i = 0; i < 4; i++){
        if(form.submitto[i].checked){
            return(true);
        }
    }
    alert("Please select a teacher to submit to.");
    return(false);
}
function checkName(form)
{
	var theField = "name";
	if(form.elements["name"].value.length < 1)
	{
		alert ("Please enter you name (only characters).");
		form.name.select();
		form.name.focus();
		return(false);
	}
	var chars = /^\D*$/;
	if(!chars.test(form.name.value))
	{
		alert ("Please enter you name (only characters).");
		form.name.select();
		form.name.focus();
		return(false);
	}
	return(true);
}
function checkFields(form)
{
	if(checkName(form) && checkTeacher(form))
		return(true);
	else
		return(false);
}
