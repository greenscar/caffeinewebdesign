DROP TABLE users;

CREATE TABLE users(
login VARCHAR(20),
firstName VARCHAR(20),
lastName VARCHAR(25),
storeNum VARCHAR (4),
secLvl VARCHAR(4),
pwd VARCHAR(15),
dateCreated VARCHAR(10),
lastLogin  VARCHAR(10));

INSERT INTO users VALUES("jsandlin", "James", "Sandlin", "99", "100", "6669", "11-7-2003", "11-7-2003");
